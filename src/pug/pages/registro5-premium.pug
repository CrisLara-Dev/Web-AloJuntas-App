extends ../layouts/default-layout4.pug
block content
    div.container
        div.my-3.row
            div.col-2.d-md-flex.d-none.justify-content-end 
                img.mt-3.img-fluid(src='./images/registro-freemium/icono.svg' alt='...')(style='width: 2.5rem; height: 4rem')
            div.col-md-8.col-12
                section.container.d-flex.justify-content-center.col-lg-10.col-md-11.col-12
                    div.mt-3.col-lg-9
                        <form class="">
                            <label for="exampleInputEmail1" class=" form-label mt-2 h6 fw400">¿Cuáles son los temas que te resultan más difíciles de abordar con tus hijas/os?</label>
                            <div class=" mt-3">
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Salud mental</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Límites y consentimiento</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Autoestima y autoconcepto</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Prevención de la violencia</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Cambios en el cuerpo</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Gestión de emociones</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Uso de la tecnología</label>
                                </div>
                                <div class="form-check mt-2 ml-1">
                                    <input class="form-check-input border-mora fs-5" type="checkbox" value="" id="flexCheckDefault">
                                    <label class="form-check-label m-1 fw400" for="flexCheckDefault">Identidad y diversidad</label>
                                </div>
                            </div>
                        </form>
                div.d-flex.justify-content-center.my-5
                    a.d-flex.text-decoration-none.m-4.justify-content-end.col-5.align-items-center(href="registro4-premium.html")
                        h6.color-black.fw400.me-2 Atrás
                        <i class="h2 fa-solid fa-circle-chevron-left color-icon3"></i>
                    a.d-flex.text-decoration-none.m-4.justify-content-start.align-items-center.col-5(href="registro6-premium.html")
                        <i class="h2 fa-solid fa-circle-chevron-right color-icon3 me-2"></i>
                        h6.color-black.fw400 Siguiente  
            div.col-2.mt-8.d-md-flex.d-none
                img.mt-3.img-fluid(src='./images/registro-freemium/icono2.svg' alt='...')(style='width: 6rem; height: 8rem')                  